  <xsd:complexType name="Cancel-Inquiry-FeeType-Response">
    <xsd:annotation>
      <xsd:documentation xml:lang="EN">
        Information about fees
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation xml:lang="EN">
          Total fee for the actual modification or cancellation of the given booking
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
          <xsd:restriction base="xsd:string">
